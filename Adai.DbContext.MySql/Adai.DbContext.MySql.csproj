<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net5.0;net6.0</TargetFrameworks>
		<Authors>Shuang Yu</Authors>
		<Company>Adai</Company>
		<Version>2.1.0.3</Version>
		<AssemblyVersion>2.1.0.3</AssemblyVersion>
		<FileVersion>2.1.0.3</FileVersion>
		<PackageProjectUrl>https://github.com/doublefish/Adai.DbContext/</PackageProjectUrl>
		<Description>数据库访问基础类库 - MySql</Description>
		<PackageReleaseNotes>无</PackageReleaseNotes>
		<PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
